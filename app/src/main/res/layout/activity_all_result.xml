<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".ResultActivity">

    <ImageView
        android:layout_width="1920px"
        android:layout_height="1080px"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:src="@drawable/background_all_result" />




    <ImageView
        android:id="@+id/content_view_heart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/content_view_heart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.75"
        app:layout_constraintHorizontal_bias="0.1"/>

    <ImageView
        android:id="@+id/status_heart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/content_view_high"
        app:layout_constraintStart_toStartOf="@+id/content_view_heart"
        app:layout_constraintEnd_toEndOf="@+id/content_view_heart"
        app:layout_constraintTop_toTopOf="@+id/content_view_heart"
        app:layout_constraintLeft_toLeftOf="@+id/content_view_heart"
        app:layout_constraintRight_toRightOf="@+id/content_view_heart"
        app:layout_constraintBottom_toBottomOf="@+id/content_view_heart"
        app:layout_constraintVertical_bias="0.09"
        app:layout_constraintHorizontal_bias="0.87"/>

    <TextView
        android:id="@+id/value_heart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="200px"
        app:layout_constraintBottom_toBottomOf="@+id/content_view_heart"
        app:layout_constraintEnd_toEndOf="@+id/content_view_heart"
        app:layout_constraintStart_toStartOf="@+id/content_view_heart"
        app:layout_constraintTop_toTopOf="@+id/content_view_heart" />


    <ImageButton
        android:id="@+id/Btn_heart_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:src="@drawable/content_btn_detail"
        app:layout_constraintBottom_toBottomOf="@+id/content_view_heart"
        app:layout_constraintEnd_toEndOf="@+id/content_view_heart"
        app:layout_constraintLeft_toLeftOf="@+id/content_view_heart"
        app:layout_constraintRight_toRightOf="@+id/content_view_heart"
        app:layout_constraintStart_toStartOf="@+id/content_view_heart"
        app:layout_constraintTop_toTopOf="@+id/content_view_heart"
        app:layout_constraintVertical_bias="0.939" />

    <ImageView
        android:id="@+id/content_view_stress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/content_view_stress"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.75" />

    <ImageView
        android:id="@+id/status_stress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/content_view_high"
        app:layout_constraintStart_toStartOf="@+id/content_view_stress"
        app:layout_constraintEnd_toEndOf="@+id/content_view_stress"
        app:layout_constraintTop_toTopOf="@+id/content_view_stress"
        app:layout_constraintLeft_toLeftOf="@+id/content_view_stress"
        app:layout_constraintRight_toRightOf="@+id/content_view_stress"
        app:layout_constraintBottom_toBottomOf="@+id/content_view_stress"
        app:layout_constraintVertical_bias="0.09"
        app:layout_constraintHorizontal_bias="0.87"/>

    <TextView
        android:id="@+id/value_stress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="200px"
        app:layout_constraintBottom_toBottomOf="@+id/content_view_stress"
        app:layout_constraintEnd_toEndOf="@+id/content_view_stress"
        app:layout_constraintStart_toStartOf="@+id/content_view_stress"
        app:layout_constraintTop_toTopOf="@+id/content_view_stress" />

    <ImageButton
        android:id="@+id/Btn_stress_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:src="@drawable/content_btn_detail"
        app:layout_constraintStart_toStartOf="@+id/content_view_stress"
        app:layout_constraintEnd_toEndOf="@+id/content_view_stress"
        app:layout_constraintTop_toTopOf="@+id/content_view_stress"
        app:layout_constraintLeft_toLeftOf="@+id/content_view_stress"
        app:layout_constraintRight_toRightOf="@+id/content_view_stress"
        app:layout_constraintBottom_toBottomOf="@+id/content_view_stress"
        app:layout_constraintVertical_bias="0.94"
        app:layout_constraintHorizontal_bias="0.5"/>



    <ImageView
        android:id="@+id/content_view_breath"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/content_view_breath"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.75"
        app:layout_constraintHorizontal_bias="0.9"/>

    <ImageView
        android:id="@+id/status_breath"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/content_view_high"
        app:layout_constraintStart_toStartOf="@+id/content_view_breath"
        app:layout_constraintEnd_toEndOf="@+id/content_view_breath"
        app:layout_constraintTop_toTopOf="@+id/content_view_breath"
        app:layout_constraintLeft_toLeftOf="@+id/content_view_breath"
        app:layout_constraintRight_toRightOf="@+id/content_view_breath"
        app:layout_constraintBottom_toBottomOf="@+id/content_view_breath"
        app:layout_constraintVertical_bias="0.09"
        app:layout_constraintHorizontal_bias="0.87"/>

    <TextView
        android:id="@+id/value_breath"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="200px"
        app:layout_constraintBottom_toBottomOf="@+id/content_view_breath"
        app:layout_constraintEnd_toEndOf="@+id/content_view_breath"
        app:layout_constraintStart_toStartOf="@+id/content_view_breath"
        app:layout_constraintTop_toTopOf="@+id/content_view_breath" />

    <ImageButton
        android:id="@+id/Btn_breath_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:src="@drawable/content_btn_detail"
        app:layout_constraintStart_toStartOf="@+id/content_view_breath"
        app:layout_constraintEnd_toEndOf="@+id/content_view_breath"
        app:layout_constraintTop_toTopOf="@+id/content_view_breath"
        app:layout_constraintLeft_toLeftOf="@+id/content_view_breath"
        app:layout_constraintRight_toRightOf="@+id/content_view_breath"
        app:layout_constraintBottom_toBottomOf="@+id/content_view_breath"
        app:layout_constraintVertical_bias="0.94"
        app:layout_constraintHorizontal_bias="0.5"/>


    <ImageButton
        android:id="@+id/Btn_home_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:padding="0dp"
        android:src="@drawable/btn_home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.98"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.03" />

</androidx.constraintlayout.widget.ConstraintLayout>